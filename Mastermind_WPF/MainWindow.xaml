<Window x:Class="Mastermind_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mastermind_WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <Ellipse Canvas.Top="30" Canvas.Left="20"  Width="30" Height="30" Name="RedPick" Fill="Red"  />
        <Ellipse Canvas.Top="70" Canvas.Left="20" Width="30" Height="30" Name="GreenPick" Fill="Green"/>
        <Ellipse Canvas.Top="110" Canvas.Left="20" Width="30" Height="30" Name="BluePick" Fill="Blue"/>
        <Ellipse Canvas.Top="150" Canvas.Left="20" Width="30" Height="30" Name="YellowPick" Fill="Yellow"/>
        <Rectangle Canvas.Top="50" Canvas.Left="150"  Width="180" Height="300" Name="Board" Fill="Brown"/>
        <Line X1="280" Y1="50" X2="280" Y2="350" Stroke="Black" StrokeThickness="1"/>



    </Canvas>
</Window>
